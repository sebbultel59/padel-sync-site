{
  "cli": {
    "version": ">= 6.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "channel": "development",
      "distribution": "internal",
      "developmentClient": true,
      "ios": {
        "simulator": false,
        "resourceClass": "default"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      }
    },
    "preview": {
      "channel": "beta",
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "resourceClass": "default",
        "distribution": "internal"
      },
      "android": {
        "distribution": "internal",
        "buildType": "apk",
        "resourceClass": "medium"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "ios": {
        "distribution": "store",
        "simulator": false,
        "buildConfiguration": "Release",
        "resourceClass": "medium"
      },
      "android": {
        "distribution": "store",
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "preview": {
      "ios": {},
      "android": {
        "track": "internal"
      }
    },
    "production": {
      "ios": {
        "appleTeamId": "F2MNK9R7Q8",
        "ascAppId": "1234567890"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "open",
        "releaseStatus": "draft"
      }
    }
  }
}